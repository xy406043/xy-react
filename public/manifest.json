{
  "manifest_version": 3,
  "name": "React Tools",
  "version": "0.0.1",
  "description": "简单工具集合",
  "offline_enabled": true,
  "background": {
    "service_worker": "back/index.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "options_page": "index.html",
  "icons": {
    "18": "img/go.png",
    "32": "img/go.png",
    "64": "img/go.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts/index.js"
      ]
    }
  ]
}