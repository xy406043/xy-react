{
    "manifest_version": 3,
    "name": "React Tools 本地版本",
    "version": "0.0.1",
    "description": "简单工具集合",
    "offline_enabled": true,
    "background": {
        "service_worker": "public/back.js"
    },
    "action": {
        "default_popup": "public/pre.html"
    },
    "icons": {
        "16": "public/img/go.png",
        "32": "public/img/go.png",
        "64": "public/img/go.png"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "public/content.js"
            ]
        }
    ]
}