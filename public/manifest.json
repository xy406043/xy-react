{
    "manifest_version": 3,
    "name": "React Tools",
    "version": "0.0.1",
    "description": "简单工具集合",
    "offline_enabled": true,
    "background": {
        "service_worker": "back.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "18": "img/18.png",
        "32": "img/18.png",
        "64": "img/18.png"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage"
    ]
}