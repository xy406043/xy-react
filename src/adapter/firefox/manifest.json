{
  "manifest_version": 2,
  "name": "React Tools",
  "version": "##version##",
  "description": "简单工具集合",
  "background": {
    "scripts": [
      "back/index.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "18": "img/go.png",
    "32": "img/go.png",
    "64": "img/go.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "js": [
        "content-scripts/index.js"
      ],
      "css": [
        "content-scripts/index.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "**"
  ]
}