{
  "name": "xy-react",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build-local": "tsc && vite build && node scripts/copy.js",
    "build": "tsc && vite build ",
    "serve": "vite preview",
    "lint:lint-staged": "lint-staged",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "prepare": "husky  install"
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,tsx,jsx,json,css,less,md}": [
      "prettier --write .",
      "pnpm run lint"
    ]
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/experimental-utils": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.4"
  }
}